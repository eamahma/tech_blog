
{{#if notes.length}}
<section>
  <h2>Your Posts</h2>
    {{#each notes as |note|}}
      <div class="card">
    <div class="card-header">
        <h4><a class="link" href="/dashboard/edit/{{id}}">{{title}}</a></h4>
    </div>
</div>
    {{/each}}

</section>
{{/if}}

<button type="submit" class="btn"><a class="link" href="/dashboard/new">+ New Post</a></button>

<script src="/javascript/add-post.js"></script>